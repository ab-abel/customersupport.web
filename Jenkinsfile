    agent any

    stages {
        stage('Build') {
            steps {
                sh 'echo we are Building at this stage..'
            }
        }
        stage('Test') {
            steps {
                sh 'echo Testing is what we are.'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo Deploying....'
            }
        }
    }
} 